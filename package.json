{
  "name": "walmart-product-manager",
  "version": "1.0.0",
  "description": "L2 Full Stack Coding Challenge",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npm run webpack-server && npm run webpack-client && npm run babel-node",
    "build": "rimraf dist/ && babel ./ --out-dir dist/ --ignore ./node_modules,./.babelrc,./package.json,./npm-debug.log --copy-files",
    "webpack-client": "webpack --config webpack.browser.config.js",
    "webpack-server": "webpack --config webpack.node.config.js",
    "babel-node": "babel-node --presets es2015 index.js"
  },
  "keywords": [
    "data",
    "walmart",
    "product"
  ],
  "author": "Syed Adil Imam",
  "license": "",
  "dependencies": {
    "axios": "^0.17.1",
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.2",
    "bootstrap": "^3.3.7",
    "css-loader": "^0.28.7",
    "datatables.net-autofill-bs": "^2.0.0",
    "datatables.net-bs": "^1.10.16",
    "datatables.net-responsive-bs": "^2.2.0",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "http-status": "^1.0.1",
    "lodash": "^4.17.4",
    "mongoose": "^4.13.4",
    "react": "^16.1.1",
    "react-bootstrap": "^0.31.5",
    "react-bootstrap-table": "^4.1.4",
    "react-dom": "^16.1.1",
    "react-router-dom": "^4.2.2"
  },
  "devDependencies": {
    "babel-cli": "6.18.0",
    "babel-core": "6.18.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "6.16.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "6.18.0",
    "nodemon": "^1.12.1",
    "parallelshell": "^3.0.2",
    "rimraf": "^2.6.2",
    "webpack": "^2.2.1",
    "webpack-node-externals": "^1.6.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2",
      "env"
    ],
    "plugins": [
      "add-module-exports"
    ]
  }
}
